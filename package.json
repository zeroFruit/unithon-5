{
  "name": "unithon",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/babel-cli/bin/babel-node.js server/index.js",
    "watch": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js server/index.js",
    "test": "set NODE_ENV=test && mocha --compilers js:babel-core/register --require babel-polyfill server/test/**/*.test.js",
    "test:watch": "nodemon --exec npm run test",
    "build": "gulp build && babel server -d dist",
    "start:prod": "pm2 start ecosystem.config.js --only api --env production",
    "stop:prod": "pm2 stop ecosystem.config.js --only api --env production",
    "restart:prod": "pm2 restart ecosystem.config.js --only api --env production"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "del": "^3.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "gulp": "^3.9.1",
    "gulp-exec": "^2.1.3",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "apn": "^2.1.5",
    "aws-sdk": "^2.92.0",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "fcm-node": "^1.2.0",
    "formidable": "^1.1.1",
    "imagemagick": "^0.1.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.4",
    "multer": "^1.3.0",
    "request": "^2.81.0",
    "validator": "^8.0.0",
    "winston": "^2.3.1"
  },
  "description": ""
}
